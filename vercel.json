{
  "version": 2,
  "builds": [
    {
      "src": "patient-registration-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "zeroConfig": true,
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 