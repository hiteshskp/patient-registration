{
  "version": 2,
  "buildCommand": "cd patient-registration-app && npm install && npm run build",
  "outputDirectory": "patient-registration-app/dist",
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
} 